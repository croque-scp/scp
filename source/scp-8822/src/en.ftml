  pointer-events: none;
  animation: remove-splash 0s linear 2s both;
}
@keyframes remove-splash {
  from { opacity: 1 } to { opacity: 0 }
}
.placebo__result {
  height: 1em;
  margin: 0;
  min-width: 5rem;
}

.alert {
  position: absolute;
  inset: 0;
  padding: 0.5rem;
  text-align: center;

  background-color: #555;
  background-image: linear-gradient(to bottom, rgb(238, 238, 238, 0.1), transparent);
